          "_sourceTheme": "cleanm4",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"carousel slide testimonials-slider testimonials2 cleanm4_testimonials2\" group=\"Testimonials\" data-bg-video=\"{{bg.type == 'video'&&bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\" plugins=\"BootstrapCarouselSwipe,mbr-testimonials-slider\" data-interval=\"false\" data-bs-interval=\"false\">\n\n\t<mbr-parameters>\n\t\t<!-- Block parameters controls (Blue \"Gear\" panel) -->\n\t\t<header>Size</header>\n\t\t<input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n\t\t<input type=\"range\" title=\"Padding Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"3\" inline>\n\t\t<input type=\"range\" title=\"Padding Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"2\" inline>\n\t\t<input type=\"range\" inline title=\"Count Items\" name=\"testimonialsSlides\" min=\"1\" max=\"20\" step=\"1\" value=\"1\">\n\t\t<input type=\"checkbox\" title=\"Autoplay\" name=\"showAutoplay\">\n\t\t<input type=\"range\" inline title=\"Autoplay Interval (ms)\" name=\"interval\" min=\"1000\" max=\"8000\" step=\"1000\" value=\"5000\" condition=\"showAutoplay\">\n\t\t<select name=\"columns\" title=\"Columns in Item\">\n\t\t\t<option value=\"1\">1</option>\n\t\t\t<option value=\"2\">2</option>\n\t\t\t<option value=\"3\" selected>3</option>\n\t\t</select>\n\t\t<header>Show / Hide</header>\n\t\t<input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked>\n\t\t<input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n\t\t<input type=\"checkbox\" title=\"Text\" name=\"showText\">\n\t\t<input type=\"checkbox\" title=\"Card Text\" name=\"showReview\" checked>\n\t\t<input type=\"checkbox\" title=\"Name\" name=\"showName\" checked>\n\t\t<input type=\"checkbox\" title=\"Profession\" name=\"showProfession\">\n\t\t<header>Colors</header>\n\t\t<input type=\"color\" title=\"Card Bg\" name=\"cardColor\" value=\"#ffffff\">\n\t\t<input type=\"color\" title=\"Icon\" name=\"iconColor\" value=\"#ffffff\">\n\t\t<input type=\"color\" title=\"Line\" name=\"lineColor\" value=\"#bbbbbb\">\n\t\t<input type=\"color\" title=\"Arrow Bg\" name=\"arrowBgColor\" value=\"#ffffff\">\n\t\t<input type=\"color\" title=\"Arrow\" name=\"arrowColor\" value=\"#ffffff\">\n\t\t<header>Background</header>\n\t\t<fieldset type=\"background\" name=\"bg\" parallax>\n\t\t\t<input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/cleanm4/components/_images/background1.jpg\">\n\t\t\t<input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n\t\t\t<input type=\"video\" title=\"Video\" value=\"https://www.youtube.com/watch?v=36YgDDJ7XSc\">\n\t\t</fieldset>\n\t\t<input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n\t\t<input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#eff2f7\" condition=\"overlay&&bg.type !== 'color'\">\n\t\t<input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay&&bg.type !== 'color'\">\n\t\t<!-- End block parameters -->\n\t</mbr-parameters>\n\n\t<div class=\"mbr-overlay\" mbr-if=\"overlay&&bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n\t</div>\n\n\t<div class=\"main\" mbr-class=\"{'container-fluid': fullWidth, 'container': !fullWidth }\">\n\t\t<h3 class=\"mbr-section-subtitle mbr-fonts-style align-center mbr-bold\" mbr-theme-style=\"display-4\" mbr-if=\"showSubtitle\" data-app-selector=\".container .mbr-section-subtitle\">-</h3>\n\n\t\t<h2 class=\"mbr-fonts-style mbr-section-title align-center mbr-bold\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".container .mbr-section-title\">繪製者</h2>\n\t\t<p class=\"mbr-text mbr-fonts-style align-center mbr-regular\" mbr-theme-style=\"display-7\" mbr-article data-app-selector=\".container .mbr-text\" data-multiline mbr-if=\"showText\">\n\t\t\tLeo duis ut diam quam nulla porttitor massa id neque. Sed risus pretium quam vulputate. Habitasse platea\n\t\t\tdictumst vestibulum rhoncus est pellentesque elit ullamcorper.</p>\n\t</div>\n\t<div class=\"carousel slide\" role=\"listbox\" data-pause=\"true\" data-keyboard=\"false\" mbr-class=\"{'container-fluid': fullWidth, 'container': !fullWidth }\" data-ride=\"{{showAutoplay ? 'carousel' : 'false'}}\" data-bs-ride=\"{{showAutoplay ? 'carousel' : 'false'}}\" data-interval=\"{{showAutoplay ? interval : 'false'}}\" data-bs-interval=\"{{showAutoplay ? interval : 'false'}}\">\n\t\t<div mbr-list mbr-list-grow=\"testimonialsSlides\" class=\"carousel-inner\">\n\t\t\t<div class=\"carousel-item\">\n\t\t\t\t<div class=\"row justify-content-center\">\n\n\t\t\t\t\t<div mbr-class=\"{'col-md-6': columns == '2', 'col-md-4': columns == '3'}\">\n\t\t\t\t\t\t<div class=\"card d-flex flex-column\">\n\t\t\t\t\t\t\t<div class=\"client-content\">\n\t\t\t\t\t\t\t\t<div class=\"service-icon\">\n\t\t\t\t\t\t\t\t\t<div class=\"service-icon-wrapper\">\n\t\t\t\t\t\t\t\t\t\t<span mbr-icon class=\"front mbr-iconfont fa-quote-left fa\" data-app-selector=\".content-link\">\n\t\t\t\t\t\t\t\t\t\t</span>\n\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t<p class=\"mbr-fonts-style mbr-text mbr-regular align-left card-text\" mbr-theme-style=\"display-4\" mbr-article data-app-selector=\".card .client-content .card-text, .mbr-section-btn\" data-multiline mbr-if=\"showReview\">\n\t\t\t\t\t\t\t\t\t畢業於國立台南藝術大學造形藝術研究所。 習慣的創作方式，主要來自對於日常生活的觀察。由於感知的敏銳，擅長藉由經驗與記憶為本，透過連結、轉譯各種她所閱讀及接收的資訊，解決自己對許多狀態及現象的提問，也因此在創作媒材上沒有固定的形式。<br>從2015年起時常佇留當時還不是蝸牛巷的蝸牛巷，同時為「蝸牛巷讀本」的寫手之一。<br></p>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t<div class=\"client-info\">\n\t\t\t\t\t\t\t\t<div class=\"client-image\">\n\t\t\t\t\t\t\t\t\t<img src=\"@PROJECT_PATH@/assets/images/timeline-20211202-152135-130x154.jpg\" title alt>\n\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t<div class=\"name-job\">\n\t\t\t\t\t\t\t\t\t<h4 class=\"mbr-fonts-style mbr-section-title mbr-semibold signature align-left\" mbr-theme-style=\"display-4\" mbr-if=\"showName\" data-app-selector=\".card .client-info .name-job .signature, .mbr-section-btn\">\n\t\t\t\t\t\t\t\t\t\t劉彥均<br>LIU,YAN-JUN</h4>\n\t\t\t\t\t\t\t\t\t<h5 class=\"mbr-fonts-style mbr-text mbr-regular profession align-left\" mbr-theme-style=\"display-4\" mbr-if=\"showProfession\" data-app-selector=\".card .client-info .name-job .profession, .mbr-section-btn\">\n\t\t\t\t\t\t\t\t\t\tChief Executive</h5>\n\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\n\t\t\t\t\t<div mbr-if=\"columns>1\" mbr-class=\"{'d-none d-md-block': columns>1, 'col-md-4': columns == '3', 'col-md-6': columns == '2'}\">\n\t\t\t\t\t\t<div class=\"card d-flex flex-column\">\n\t\t\t\t\t\t\t<div class=\"client-content\">\n\t\t\t\t\t\t\t\t<div class=\"service-icon\">\n\t\t\t\t\t\t\t\t\t<div class=\"service-icon-wrapper\">\n\t\t\t\t\t\t\t\t\t\t<span mbr-icon class=\"front mbr-iconfont fa-quote-left fa\" data-app-selector=\".content-link\"></span>\n\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t<p class=\"mbr-fonts-style mbr-text mbr-regular align-left card-text\" mbr-theme-style=\"display-4\" mbr-article data-app-selector=\".card .client-content .card-text, .mbr-section-btn\" data-multiline mbr-if=\"showReview\">藝術創作者／策展人／企劃人\n<br>陳德聚堂門神作者潘岳雄先生之學生&nbsp;<br>台北藝術大學美術系\n<br>台大人類學研究所博士班\n<br>現職 蓬萊齋藝術創作室\n\t\t\t\t\t\t\t\t</p>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t<div class=\"client-info\">\n\t\t\t\t\t\t\t\t<div class=\"client-image\">\n\t\t\t\t\t\t\t\t\t<img src=\"@PROJECT_PATH@/assets/images/118772804-1433265617063136-1902883172152123280-n-130x130.jpg\" title alt>\n\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t<div class=\"name-job\">\n\t\t\t\t\t\t\t\t\t<h4 class=\"mbr-fonts-style mbr-section-title mbr-semibold signature align-left\" mbr-theme-style=\"display-4\" mbr-if=\"showName\" data-app-selector=\".card .client-info .name-job .signature, .mbr-section-btn\">黃稚原<br>HUANG,ZHI-YUAN</h4>\n\t\t\t\t\t\t\t\t\t<h5 class=\"mbr-fonts-style mbr-text mbr-regular profession align-left\" mbr-theme-style=\"display-4\" mbr-if=\"showProfession\" data-app-selector=\".card .client-info .name-job .profession, .mbr-section-btn\">\n\t\t\t\t\t\t\t\t\t\tChief Executive</h5>\n\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\n\t\t\t\t\t<div mbr-if=\"columns>2\" mbr-class=\"{'d-none d-md-block': columns>2, 'col-md-4': columns == '3', 'col-md-6': columns == '2'}\">\n\t\t\t\t\t\t<div class=\"card d-flex flex-column\">\n\t\t\t\t\t\t\t<div class=\"client-content\">\n\t\t\t\t\t\t\t\t<div class=\"service-icon\">\n\t\t\t\t\t\t\t\t\t<div class=\"service-icon-wrapper\">\n\t\t\t\t\t\t\t\t\t\t<span mbr-icon class=\"front mbr-iconfont fa-quote-left fa\" data-app-selector=\".content-link\"></span>\n\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t<p class=\"mbr-fonts-style mbr-text mbr-regular align-left card-text\" mbr-theme-style=\"display-4\" mbr-article data-app-selector=\".card .client-content .card-text, .mbr-section-btn\" data-multiline mbr-if=\"showReview\">\n\t\t\t\t\t\t\t\t\t從2016年開始參與台南大宮町藝術浸潤計畫 《蝸牛巷慢慢走》社區營造-美術設計\n與執行隊員，2012年移居台南至目前成為蝸牛巷住民邁入第十年。\n工作專業培育以美術，擅長敦親睦鄰並適度讓自己保持貧窮。擁有一台手工攤車\n「綠豆皮」，執行“台南的粉角綠豆湯之味”任務，並且落實”勞動獲得自由”與創作合而為一的小生意。必要時沉迷於設計工作，是靈活有趣有效率的偏向。可以執行繪圖、手工製作、裝置、影片，技能的通道隨時都在開拓，各項雖不能稱之為大師級的專精，卻飽含技術上解決問題的熱忱與學習力。另外在意時間結構。培育於復興美工、高雄大學。</p>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t<div class=\"client-info\">\n\t\t\t\t\t\t\t\t<div class=\"client-image\">\n\t\t\t\t\t\t\t\t\t<img src=\"@PROJECT_PATH@/assets/images/27d932f3-c1f9-4daa-8498-7bfcab18dcff-130x98.jpeg\" title alt>\n\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t<div class=\"name-job\">\n\t\t\t\t\t\t\t\t\t<h4 class=\"mbr-fonts-style mbr-section-title mbr-semibold signature align-left\" mbr-theme-style=\"display-4\" mbr-if=\"showName\" data-app-selector=\".card .client-info .name-job .signature, .mbr-section-btn\">陳怡妏<br>CHEN,YI-WEN</h4>\n\t\t\t\t\t\t\t\t\t<h5 class=\"mbr-fonts-style mbr-text mbr-regular profession align-left\" mbr-theme-style=\"display-4\" mbr-if=\"showProfession\" data-app-selector=\".card .client-info .name-job .profession, .mbr-section-btn\">\n\t\t\t\t\t\t\t\t\t\tChief Executive</h5>\n\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t\t\n\t\t</div>\n\n\t\t<!-- <div class=\"carousel-indicators\">\n            <li data-slide-to=\"0\" data-bs-slide-to=\"0\" class=\"active\"></li>\n            <li mbr-if=\"testimonialsSlides> 1\" data-slide-to=\"1\" data-bs-slide-to=\"1\"></li>\n            <li mbr-if=\"testimonialsSlides> 2\" data-slide-to=\"2\" data-bs-slide-to=\"2\"></li>\n            <li mbr-if=\"testimonialsSlides> 3\" data-slide-to=\"3\" data-bs-slide-to=\"3\"></li>\n            <li mbr-if=\"testimonialsSlides> 4\" data-slide-to=\"4\" data-bs-slide-to=\"4\"></li>\n            <li mbr-if=\"testimonialsSlides> 5\" data-slide-to=\"5\" data-bs-slide-to=\"5\"></li>\n            <li mbr-if=\"testimonialsSlides> 6\" data-slide-to=\"6\" data-bs-slide-to=\"6\"></li>\n            <li mbr-if=\"testimonialsSlides> 7\" data-slide-to=\"7\" data-bs-slide-to=\"7\"></li>\n            <li mbr-if=\"testimonialsSlides> 8\" data-slide-to=\"8\" data-bs-slide-to=\"8\"></li>\n            <li mbr-if=\"testimonialsSlides> 9\" data-slide-to=\"9\" data-bs-slide-to=\"9\"></li>\n        </div> -->\n\n\t\t<div class=\"carousel-controls\">\n\t\t\t<a class=\"carousel-control-prev\" role=\"button\" data-slide=\"prev\" data-bs-slide=\"prev\">\n\t\t\t\t<span aria-hidden=\"true\" class=\"mbr-iconfont mbrib-left\"></span>\n\t\t\t\t<span class=\"sr-only visually-hidden\">Previous</span>\n\t\t\t</a>\n\n\t\t\t<a class=\"carousel-control-next\" role=\"button\" data-slide=\"next\" data-bs-slide=\"next\">\n\t\t\t\t<span aria-hidden=\"true\" class=\"mbr-iconfont mbrib-right\"></span>\n\t\t\t\t<span class=\"sr-only visually-hidden\">Next</span>\n\t\t\t</a>\n\t\t</div>\n\t</div>\